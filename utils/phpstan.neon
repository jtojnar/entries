includes:
	- ../vendor/phpstan/phpstan-nette/extension.neon
	- ../vendor/phpstan/phpstan-nette/rules.neon
	- ../vendor/nextras/orm-phpstan/extension.neon
parameters:
	level: max
	paths:
		- ../app
		- ../tests
		- ../www
	checkMissingIterableValueType: false
	treatPhpDocTypesAsCertain: false
	checkGenericClassInNonGenericObjectType: false
	ignoreErrors:
		- '(Call to an undefined method App\\Components\\TeamForm::addDynamic\(\))'
		# https://github.com/phpstan/phpstan/issues/6402
		- '(Readonly property App\\Model\\Configuration\\CategoryData::\$allCategories is already assigned.)'
		# https://github.com/phpstan/phpstan/issues/8954
		- '(Method App\\Model\\Configuration\\Helpers::ensureSubclassOf\(\) should return class-string<V>\|null but returns class-string<U>.)'
